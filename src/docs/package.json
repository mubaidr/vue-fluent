{
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.4",
    "@vue/cli-plugin-pwa": "^3.1.1",
    "@vue/cli-service": "^3.1.3",
    "@vue/eslint-config-standard": "^3.0.4",
    "@vue/eslint-config-prettier": "^4.0.0",
    "eslint": "^5.9.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "@vue/standard",
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "globals": {
      "Prism": true,
      "Vue": true,
      "document": true,
      "window": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "root": true,
    "rules": {
      "no-console": 1
    }
  },
  "eslintIgnore": [
    "src/assets/"
  ],
  "name": "docs",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "dev": "npm run serve",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve"
  },
  "version": "0.1.0"
}
